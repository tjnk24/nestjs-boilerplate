{
  "name": "nestjs-boilerplate",
  "version": "0.0.2",
  "description": "nestjs-boilerplate",
  "author": "tjnk24",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "lint": "npm-run-all lint:*",
    "lint:tsc": "npx tsc -p tsconfig.json --skipLibCheck",
    "lint:src": "npx eslint --ext=.ts ./",
    "postinstall": "npx husky install",
    "test": "npm-run-all lint",
    "build-apidocs": "npm run clean & npx @openapitools/openapi-generator-cli generate & cd apiDocs & npm publish",
    "clean": "rm -r ./apiDocs",
    "precommit":"uver && git add -A"
  },
  "dependencies": {
    "@nestjs/common": "10.0.0",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.0.0",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/platform-express": "10.0.0",
    "@nestjs/sequelize": "10.0.0",
    "@nestjs/swagger": "7.1.8",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cookie-parser": "1.4.6",
    "dotenv": "16.3.1",
    "express-fingerprint": "1.2.2",
    "lodash": "4.17.21",
    "pg": "8.11.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "sequelize": "6.32.1",
    "yaml": "2.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.0.0",
    "@nestjs/schematics": "10.0.0",
    "@openapitools/openapi-generator-cli": "2.9.0",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.17",
    "@types/lodash": "4.14.197",
    "@types/node": "20.3.1",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "eslint": "8.42.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-eslint-plugin": "5.1.0",
    "eslint-plugin-import": "2.27.5",
    "husky": "8.0.3",
    "npm-run-all": "4.1.5",
    "source-map-support": "0.5.21",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3",
    "uver-cli": "1.1.0"
  }
}
